{
	"compilerOptions": {
		"target": "es6",
		"module": "es6",
		"moduleResolution": "node",
		"declaration": false,
		"noImplicitAny": false,
		"jsx": "react",
		"sourceMap": true,
		"noLib": false,
		"suppressImplicitAnyIndexErrors": true,
		"baseUrl": "./src/",
		"types": ["@emotion/react/types/css-prop"]
	},
	"compileOnSave": false,
	"allowSyntheticDefaultImports": true,
	"exclude": ["node_modules"]
}
